{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**", "build/**"]
    },
    "build#mobile": {
      "cache": false,
      "outputs": []
    },
    "start": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "lint#mobile": {
      "cache": false,
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "typecheck#mobile": {
      "cache": false,
      "outputs": []
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**"]
    }
  }
}
